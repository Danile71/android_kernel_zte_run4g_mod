.PHONY: met-cmd

ifeq ($(KDIR),)
$(error Error! You should execute "make" in parent directory)
endif

MET_APP_SRC=$(PWD)/met-cmd

met-cmd:
	cd $(ALPS); $(ENV_SH) mmm $(MET_APP_SRC); cd -
	cp -f $(ALPS)/out/target/product/$(PROJECT_SHORT)/system/bin/met-cmd $$PWD

clean:
	touch *.c *.h
	rm -f met-cmd

