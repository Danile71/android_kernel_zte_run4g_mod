
.PHONY: met-tag

ifeq ($(KDIR),)
$(error Error! You should execute "make" in parent directory)
endif

export MET_APP_SRC=$(PWD)/met-tag

all:
	cd $(ALPS); $(ENV_SH) mmm $(MET_APP_SRC)/test; cd -
	cp -f $(ALPS)/out/target/product/$(PROJECT_SHORT)/system/bin/mtag* $$PWD

clean:
	touch *.c
	rm -f mtag_test*

