<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.mediatek.op02.plugin"
          android:versionCode="1"
          android:versionName="1.0">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_MEDIA_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <uses-sdk android:minSdkVersion="5"/>

    <!-- Add your plug-in service here -->
    <application android:label="OperatorPlugin">
        <service android:name=".Op02MovieExtension"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.gallery3d.ext.IMovieExtension"/>
            </intent-filter>
            <meta-data
                    android:name="class"
                    android:value="com.mediatek.gallery3d.plugin.Op02MovieExtension"/>
        </service>

        <activity android:name="com.mediatek.gallery3d.plugin.VideoSettingsActivity"
                android:label="@string/settings"
                android:exported="true"
                android:configChanges="orientation|keyboardHidden|screenSize|mnc|mcc">
        </activity>

        <service android:name=".Op02ImageOptions"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.gallery3d.ext.IImageOptions"/>
            </intent-filter>
            <meta-data
                    android:name="class"
                    android:value="com.mediatek.gallery3d.ext.image.Op02ImageOptions"/>
        </service>

        <!-- Mms Plugin Start--> 
        <service android:name=".Op02MmsConfig"
                  android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.mms.ext.IMmsConfigExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.mms.plugin.Op02MmsConfigExt" />
        </service>

        <service android:name=".Op02MmsConversation"
                  android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.mms.ext.IMmsConversationExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.mms.plugin.Op02MmsConversationExt" />
        </service>
        <service android:name=".Op02MmsFeatureManager"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.mms.ext.IMmsFeatureManagerExt"/>
            </intent-filter>
            <meta-data
                    android:name="class"
                    android:value="com.mediatek.mms.plugin.Op02MmsFeatureManagerExt"/>
        </service>
        <!-- Mms Plugin End-->
        <service
            android:name=".ContactsPlugin"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.android.contacts.ext.IOPContactPlugin" />
            </intent-filter>
            <meta-data
                android:name="class"
                android:value="com.mediatek.contacts.plugin.OP02ContactsPlugin" />
        </service>

        <!-- Calendar Plugin Start -->
        <service android:name=".TCLunar"
                  android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.calendar.ext.ILunarExtension" />
            </intent-filter>
            <meta-data
                android:name="class" 
                android:value="com.mediatek.calendar.plugin.TCLunarPlugin" />
            <meta-data
                android:name="version"
                android:value="1.0.2" />
        </service>
        <!-- Calendar Plugin End -->

       <!-- Setting Plugin Start-->
       <service 
            android:name=".IStatusGeminiExt" 
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.settings.ext.IStatusGeminiExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.settings.plugin.OP02StatusGeminiExt" />
        </service>
        <service 
            android:name=".IDeviceInfoSettingsExt" 
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.settings.ext.IDeviceInfoSettingsExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.settings.plugin.OP02DeviceInfoSettingsExt" />
        </service>
        <service 
            android:name=".ISimManagementExt" 
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.settings.ext.ISimManagementExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.settings.plugin.OP02SimManagementExt" />
        </service>
        <service 
            android:name=".ISettingsMiscExt" 
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.settings.ext.ISettingsMiscExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.settings.plugin.OP02SettingsMiscExt" />
        </service>
        <service 
            android:name=".IReplaceApnProfileExt" 
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.settings.ext.IReplaceApnProfileExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.settings.plugin.OP02ReplaceApnProfileExt" />
        </service>
        <service 
            android:name=".IApnSettingsExt" 
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.settings.ext.IApnSettingsExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.settings.plugin.OP02ApnSettingsExt" />
        </service>
        <service
            android:name=".IDataUsageSummaryExt"
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.settings.ext.IDataUsageSummaryExt" />
            </intent-filter>
            <meta-data
                android:name="class"
                android:value="com.mediatek.settings.plugin.OP02DataUsageSummaryExt" />
        </service>
        <!-- Setting Plugin End-->

        <!-- SystemUI Plugin Start-->
        <service android:name=".Op02StatusBarPlugin"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.systemui.ext.IStatusBarPlugin"/>
            </intent-filter>
            <meta-data
                    android:name="class"
                    android:value="com.mediatek.systemui.plugin.Op02StatusBarPlugin"/>
            <meta-data android:name="version" android:value="1.0.0"/>
        </service>
        <!-- SystemUI Plugin End-->

        <!-- Keyguard Plugin Start-->
        <service android:name=".OP02EmergencyButtonExt"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.keyguard.ext.IEmergencyButtonExt"/>
            </intent-filter>
            <meta-data
                    android:name="class"
                    android:value="com.mediatek.keyguard.plugin.OP02EmergencyButtonExt"/>
            <meta-data android:name="version" android:value="1.0.0"/>
        </service>
        <service android:name=".OP02CarrierTextExt"
                 android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.keyguard.ext.ICarrierTextExt"/>
            </intent-filter>
            <meta-data
                    android:name="class"
                    android:value="com.mediatek.keyguard.plugin.OP02CarrierTextExt"/>
            <meta-data android:name="version" android:value="1.0.0"/>
        </service>
        <!-- Keyguard Plugin End-->

        <!-- FileManager Plugin Start -->
        <service
            android:name=".FileManagerIconExtension"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.mediatek.filemanager.ext.IIconExtension" />
            </intent-filter>
            <meta-data
                android:name="class"
                android:value="com.mediatek.filemanager.plugin.SystemIconExtension" />
        </service>
        <!-- FileManager Plugin End -->
        <!-- Browser Plugin Start-->
        <service android:name=".Op02BrowserBookmarkExt"
                  android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.browser.ext.IBrowserBookmarkExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.browser.plugin.Op02BrowserBookmarkExt" />
        </service>
        <service android:name=".Op02BrowserDownloadExt"
                  android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.browser.ext.IBrowserDownloadExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.browser.plugin.Op02BrowserDownloadExt" />
        </service>
        <service android:name=".Op02BrowserSettingExt"
                  android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.browser.ext.IBrowserSettingExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.browser.plugin.Op02BrowserSettingExt" />
        </service>
        <!-- Browser Plugin End-->
        
        <!-- DownloadManager Plugin Start-->
        <service android:name=".Op02DownloadProviderFeatureExt"
                  android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.downloadmanager.ext.IDownloadProviderFeatureExt" />
            </intent-filter>
            <meta-data 
                android:name="class" 
                android:value="com.mediatek.downloadmanager.plugin.Op02DownloadProviderFeatureExt" />
        </service>
        <!-- DownloadManager Plugin End-->

        <!-- Phone Plugin Start-->
        <service
            android:name="com.mediatek.phone.plugin.PhonePlugin"
            android:exported="true">
            <intent-filter>
                <action android:name="com.mediatek.phone.ext.IPhonePlugin"/>
            </intent-filter>
            <meta-data
                android:name="class"
                android:value="com.mediatek.phone.plugin.OP02PhonePlugin"/>
            <meta-data android:name="version" android:value="1.0.0"/>
            </service>
        <!-- Phone Plugin End-->
    </application>
</manifest>
